        FKART.recommendations.carousel.load({
            carouselContainer:'#recently-viewed',
                itemsPerSlide: 4,
                noOfVisibleItems: 1,
                productsPerScroll: 1,
            title: "You Recently Viewed",
            url: '/dynamic/recommendation/carousel/getRecentBrowseHistory',
            typeOfRequest: 'GET',
            requestParams: {showCashback: '0' ,productIds:"",moduleId: 'historyFooter',pageId:"sp",vertical:"",limit:"",hideLastViewed:""}
        });

        setTimeout(function(){
            FKART.recommendations.carousel.load({
                carouselContainer:'#browse-history-recomm',
                itemsPerSlide: 1,
                noOfVisibleItems: 5,
                productsPerScroll: 5,
                title: "Recommendations Based On Your Browsing History",
                showLoader: true,
                cssClass: "fk-history-item-carousel",
                typeOfRequest: 'GET',
                url: '/dynamic/recommendation/carousel/getBrowseHistoryBasedRecommendations',
                requestParams: {reco_ordering_key:"",reco_ordering_value:"",productIds:"",moduleId: 'personalhistoryFooter',pageId:"sp",vertical:"",useCrossVertical:"",filterVerticalValue:"",limit:"",hideLastViewed:""}
            });
        }, 400);

        $('#browse-history-recomm, #recently-viewed').bind('response-failed', function() {
            $('#recentBrowseHistoryRecomm').hide();
        });

</script>

